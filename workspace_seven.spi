define :drums do
  sync :tick
  with_fx :echo do
  sample :drum_heavy_kick, rate: 1
  sleep 1
  end
  
end

define :synths do
  wait :tick
  with_fx :slicer, rate: 8 do
   sample :bass_drop_c, start: 0, finish: 0.2, release: 0.1
  end
end

in_thread(name: :drums){loop{drums}}
in_thread(name: :synths){loop{synths}}
 