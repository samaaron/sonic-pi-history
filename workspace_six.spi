use_synth :prophet

in_thread do
  with_fx :reverb do
    loop do
      sample :elec_blip2, rate: 2
      sleep 0.25
    end
  end
end


with_fx :slicer, wave: 1, phase: 0.125 do
  loop do
    with_synth :beep do
      play [30, 35].choose, release: 2
    end
    play 42, res: 0.3
    sleep 2
  end
end