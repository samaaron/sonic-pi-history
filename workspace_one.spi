use_synth :prophet 
use_transpose 5
use_synth_defaults release: 0.2, cutoff: lambda{rrand(60, 120)}
use_bpm 60
with_fx :echo, delay: 0.25 do
  with_fx :reverb do
    in_thread do

      play_pattern_timed [53, 76, 79]* 4, [0.25, 0.5, 0.25, 0.25]
    end
    sleep 0.5
    play_pattern_timed [52, 69, 49]* 4, [0.25, 0.5, 0.25, 0.25]

  end
end